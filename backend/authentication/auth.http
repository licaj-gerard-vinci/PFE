@baseUrl = http://localhost:8000

### register dans admin
POST {{baseUrl}}/auth/register/
Content-Type: application/json

{
  "nom":"testNom",
  "prenom":"testPrenom",
  "email":"julien@admin.com",
  "mdp":"ipl"
}

### Login avec Julien
POST {{baseUrl}}/auth/login/
Content-Type: application/json

{
  "email":"julien@admin.com",
  "mdp":"ipl"
}
> {%
  client.global.set("julienToken", response.body.access);
%}

